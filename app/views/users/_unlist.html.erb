<% list = List.find_by(course_id: @course.id) %>
<%= link_to "/lists/#{list.id}/destroy",
            method: :delete,
            class: 'unlist',
            remote: true do %>
  <% if current_user == @course.user %>
    <div class="list-btn-unlist"><i class="fas fa-list-alt"></i> ブックマーク済み</div>
  <% else %>
    <div class="list-btn-unlist"><i class="fas fa-utensils"></i> ブックマーク済み</div>
  <% end %>
<% end %>
